type Query{
    getBook : [Book]
    getById(id:ID!): Book
    getByLimit(number:Int):[Book]
}
type Mutation{
    addBook(name:String!):Book!
    updateBook(id:ID!,name:String!):Book
    deleteBook(id:ID!): String
    deleteByName(name:String!):String
}
type Book{
    name:String!
    memberId: ID
}
